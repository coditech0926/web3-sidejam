{
  "name": "stratego-four",
  "version": "0.1.0",
  "private": true,
  "author": "Web3Studio Developers <web3studio@consensys.net>",
  "scripts": {
    "start": "yarn start:migrate && yarn start:web",
    "start:clean": "yarn clean",
    "start:web": "react-scripts start",
    "start:migrate": "truffle migrate --reset --skip-dry-run --network=pantheon --contracts_build_directory=$(pwd)/src/build/contracts",
    "clean": "rm -rf src/build",
    "pantheon": "docker-compose up -d",
    "pantheon:down": "docker-compose down --remove-orphans -v ",
    "build": "react-scripts build",
    "build:truffle": "yarn clean && yarn truffle build --contracts_build_directory=$(pwd)/src/build/contracts",
    "test": "yarn build:truffle && CI=true yarn react-scripts test",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "bip39": "^2.2.0",
    "drizzle": "^1.4.0",
    "drizzle-react": "^1.2.0",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^5.2.0",
    "ethereumjs-wallet": "^0.6.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.0",
    "reselect": "^4.0.0",
    "rimble-ui": "^0.8.0",
    "styled-components": "^4.2.0",
    "web3-provider-engine": "^15.0.0"
  },
  "devDependencies": {
    "truffle-hdwallet-provider": "^1.0.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
